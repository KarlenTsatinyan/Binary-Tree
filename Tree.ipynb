    "def max_sum(node, path):\n",
    "    ls = rs = 0\n",
    "    lp = rp = path\n",
    "    if node.left:\n",
    "        ls, lp = max_sum(node.left, path + ['l'])\n",
    "    if node.right:\n",
    "        rs, rp = max_sum(node.right, path + ['r'])\n",
    "    ls += node.data\n",
    "    rs += node.data\n",
    "    return (ls, lp) if ls > rs else (rs, rp)\n",
    "print(max_sum(root, []))"
