   "source": [
    "### Function to convert from List to Tree (Level Order Fashion)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 41,
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "0 0 9 2 5 1 6 3 0 "
